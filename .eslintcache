= (props) => {\r\n  const [form, setForm] = useState(false);\r\n  const [errorSending, setErrorSending] = useState(false);\r\n\r\n  const { isLoading, dataSent, sendRequest } = useHttpRequest();\r\n\r\n  const orderHandler = () => {\r\n    setForm(true);\r\n  };\r\n\r\n  const cartCtx = useContext(CartContext);\r\n  const total = `$${cartCtx.totalAmount.toFixed(2)}`;\r\n  const hasItems = cartCtx.items.length > 0;\r\n\r\n  const removeHandler = (id) => {\r\n    cartCtx.removeItem(id);\r\n  };\r\n\r\n  const addHandler = (item) => {\r\n    cartCtx.addItem({ ...item, amount: 1 });\r\n  };\r\n\r\n  const sentDataHandler = async (userData) => {\r\n    sendRequest({\r\n      url: `https://food-react-47e6c-default-rtdb.firebaseio.com/orders.json`,\r\n      method: \"POST\",\r\n      body: { user: userData, orderedItem: cartCtx.items },\r\n    });\r\n  };\r\n\r\n  const cartItems = cartCtx.items.map((item, i) => (\r\n    <CartItem\r\n      onRemove={removeHandler.bind(null, item.id)}\r\n      onAdd={addHandler.bind(null, item)}\r\n      key={cartCtx.items[i].id}\r\n      amount={cartCtx.items[i].amount}\r\n      price={cartCtx.items[i].price}\r\n      name={cartCtx.items[i].name}\r\n    />\r\n  ));\r\n\r\n  const modalButtons = (\r\n    <div className={classes.actions}>\r\n      <button onClick={props.onCloseModal} className={classes[\"button--alt\"]}>\r\n        Close\r\n      </button>\r\n      {hasItems && (\r\n        <button onClick={orderHandler} className={classes.button}>\r\n          Order\r\n        </button>\r\n      )}\r\n    </div>\r\n  );\r\n\r\n  const cartModalContent = (\r\n    <React.Fragment>\r\n      <ul className={classes[\"cart-items\"]}>{cartItems}</ul>\r\n      <div className={classes.total}>\r\n        <span>Total amount</span>\r\n        <span> {total}</span>\r\n      </div>\r\n      {form && (\r\n        <Checkout\r\n          onValidUserData={sentDataHandler}\r\n          onCancel={props.onCloseModal}\r\n        />\r\n      )}\r\n      {!form && modalButtons}\r\n    </React.Fragment>\r\n  );\r\n\r\n  const submitingOrder = <p> Your order is being sending ...</p>;\r\n  const orderSentModal = (\r\n    <React.Fragment>\r\n      <p>Your order has been sent successfully 😊</p>\r\n      <div className={classes.actions}>\r\n        <button onClick={props.onCloseModal} className={classes[\"button--alt\"]}>\r\n          Close\r\n        </button>\r\n      </div>\r\n    </React.Fragment>\r\n  );\r\n\r\n  const errorContentModal = (\r\n    <React.Fragment>\r\n      <p>There has been an error, please check your internet connection!</p>\r\n      <div className={classes.actions}>\r\n        <button onClick={props.onCloseModal} className={classes[\"button--alt\"]}>\r\n          Close\r\n        </button>\r\n      </div>\r\n    </React.Fragment>\r\n  );\r\n\r\n  return (\r\n    <Modal onClose={props.onCloseModal}>\r\n      {!isLoading && !dataSent && cartModalContent}\r\n      {isLoading && submitingOrder}\r\n      {dataSent && !isLoading && orderSentModal}\r\n      {!dataSent && isLoading && errorSending && errorContentModal}\r\n    </Modal>\r\n  );\r\n};\r\n\r\nexport default Cart;\r\n","C:\\Users\\gccc1\\Desktop\\react-complete-guide\\food-order-app\\01-starting-project\\src\\UI\\modal\\Modal.js",[],"C:\\Users\\gccc1\\Desktop\\react-complete-guide\\food-order-app\\01-starting-project\\src\\store\\CartContext.js",[],"C:\\Users\\gccc1\\Desktop\\react-complete-guide\\food-order-app\\01-starting-project\\src\\store\\store-context.js",[],"C:\\Users\\gccc1\\Desktop\\react-complete-guide\\food-order-app\\01-starting-project\\src\\components\\Layout\\CartItem\\CartItem.js",[],"C:\\Users\\gccc1\\Desktop\\react-complete-guide\\food-order-app\\01-starting-project\\src\\components\\Layout\\Cart\\Checkout.js",[],"C:\\Users\\gccc1\\Desktop\\react-complete-guide\\food-order-app\\01-starting-project\\src\\custom hooks\\useRequest.js",[],{"ruleId":"83","severity":1,"message":"84","line":10,"column":22,"nodeType":"85","messageId":"86","e